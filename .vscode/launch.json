{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test Random Large",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/run.py",
            "console": "integratedTerminal",
            "args": ["configs/test_random_large.yaml"],
            "python": "${workspaceFolder}/.venv/bin/python",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
